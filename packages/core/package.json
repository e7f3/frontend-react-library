{
  "name": "@e7f3/core",
  "version": "1.0.1",
  "description": "Core utilities and shared logic",
  "type": "module",
  "packageManager": "yarn@4.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/e7f3/frontend-react-library",
    "directory": "packages/core"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "prepublishOnly": "yarn clean && yarn build",
    "test": "jest --config ./jest.config.cjs",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "axios": "^1.13.2"
  },
  "devDependencies": {
    "@e7f3/build": "workspace:^",
    "@reduxjs/toolkit": "^2.5.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.1",
    "eslint": "^9.19.0",
    "jest": "^29.7.0",
    "react": "^19.0.0",
    "react-redux": "^9.2.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": "^2.5.1",
    "react": "^19.0.0",
    "react-redux": "^9.2.0",
    "typescript": "^5.7.3"
  }
}
