{
  "name": "@e7f3/ui",
  "version": "1.0.1",
  "description": "Shared UI components library",
  "type": "module",
  "packageManager": "yarn@4.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/e7f3/frontend-react-library",
    "directory": "packages/ui"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "scripts": {
    "build": "tsup && copyfiles -u 1 \"shared/**/*.scss\" \"shared/**/*.css\" dist",
    "clean": "rm -rf dist",
    "prepublishOnly": "yarn clean && yarn build",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "lint:scss": "stylelint \"**/*.scss\"",
    "storybook": "sb dev",
    "storybook:build": "sb build",
    "test:unit": "jest --config ./jest.config.cjs",
    "test:unit:watch": "jest --watch --config ./jest.config.cjs"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@e7f3/core": "workspace:^"
  },
  "devDependencies": {
    "@e7f3/build": "workspace:^",
    "@babel/core": "^7.26.7",
    "@babel/preset-env": "^7.26.7",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/preview-api": "^8.6.14",
    "@storybook/react-webpack5": "^10.0.0",
    "@table-library/react-table-library": "^4.1.12",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/copyfiles": "^2",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack": "^5.28.5",
    "babel-loader": "^9.2.1",
    "copyfiles": "^2.4.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.19.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "jest": "^29.7.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.0.0",
    "sass": "^1.94.0",
    "sass-loader": "^16.0.0",
    "storybook": "^10.0.0",
    "style-loader": "^4.0.0",
    "stylelint": "^16.14.1",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "tsup": "^8.5.1",
    "typescript": "^5.7.3",
    "webpack": "^5.97.1"
  },
  "peerDependencies": {
    "@table-library/react-table-library": "^4.1.12",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.0.0",
    "typescript": "^5.7.3"
  }
}